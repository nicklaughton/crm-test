{
    "name": "nick-crm-2024-2",
    "version": "0.0.3",
    "description": "Nick Crm 2024 2",
    "keywords": [
        "loopback-application",
        "loopback"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "prebuild": "mkdir -p dist && npm run clean",
        "build": "lb-tsc",
        "prebuild:watch": "npm run clean",
        "build:watch": "lb-tsc --watch --preserveWatchOutput",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
        "prettier:check": "npm run prettier:cli -- -l",
        "prettier:fix": "npm run prettier:cli -- --write",
        "eslint": "lb-eslint --report-unused-disable-directives .",
        "eslint:fix": "npm run eslint -- --fix",
        "docker:build": "docker build -t example .",
        "docker:run": "docker run -p 3000:3000 -d example",
        "migrate": "node ./dist/migrate",
        "openapi-spec": "node ./dist/openapi-spec",
        "start": "node .",
        "clean": "lb-clean $(find dist -path \"*/*\" ! -path \"*processes*\") *.tsbuildinfo .eslintcache",
        "version": "echo $npm_package_version",
        "test": "jasmine --config=jasmine.json"
    },
    "repository": {
        "type": "git"
    },
    "author": "",
    "license": "",
    "files": [
        "README.md",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "dependencies": {
        "@loopback/boot": "^5.0.3",
        "@loopback/core": "~4.0.3",
        "@loopback/openapi-v3": "~8.0.3",
        "@loopback/repository": "~5.1.0",
        "@loopback/rest": "~12.0.3",
        "@loopback/rest-explorer": "~5.0.3",
        "@loopback/service-proxy": "~5.0.3",
        "@loopback/authorization": "~0.12.3",
        "@loopback/authentication": "~9.0.3",
        "@loopback/health": "~0.11.3",
        "tslib": "^2.0.0",
        "debug": "~4.3.0",
        "async": "~3.2.0",
        "source-map-support": "^0.5.19",
        "replace-in-file": "~6.2.0",
        "compression": "~1.7.4",
        "helmet": "~4.6.0",
        "adm-zip": "~0.5.9",
        "express-unless": "~2.1.3",
        "uuid": "~8.3.2",
        "@loopback/repository-json-schema": "~6.0.3",
        "ajv": "^8.0.1",
        "change-case": "~4.1.2",
        "fs-extra": "~9.1.0",
        "@loopback/security": "~0.8.3",
        "got": "~11.8.3",
        "jwks-rsa": "~1.12.0",
        "express-jwt": "~6.0.0",
        "express-jwt-authz": "~2.4.0",
        "swagger-ui-dist": "~5.3.1",
        "loopback-connector-postgresql": "~5.2.1",
        "pg-connection-string": "~2.4.0"
    },
    "devDependencies": {
        "@loopback/build": "~9.0.3",
        "@loopback/testlab": "~5.0.3",
        "@types/node": "^10.17.60",
        "@loopback/eslint-config": "^13.0.0",
        "eslint": "^8.36.0",
        "typescript": "~4.6.3",
        "jasmine": "~3.8.0",
        "@types/jasmine": "~3.8.1",
        "ts-node": "~9.1.1",
        "sinon": "~7.5.0",
        "@types/lodash": "4.17.5"
    }
}
